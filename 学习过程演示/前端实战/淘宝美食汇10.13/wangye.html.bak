      <title>我的第一个网页</title>
      
	  <style>
	     html,body,div,ul,li,img{
		     margin:0;
			 padding:0;
            }
	     #cate{
		     
		     width:1200px;
			 height:360px;
			 margin:100px auto;
			 border:2px solid #dec2c2;
			 box-shadow:1px 1px 9px #222;
		 }
		 #cate ul{
		     display:flex;
			 justify-content:space-around;
			 align-items:center;
			 flex-wrap:wrap;
			 width:100%;
			 height:100%;
		     list-style:none;
		 }
		 #cate ul li{
		     position:relative;
		     width:360px;
			 height:135px;
			 border:2px solid #dec2c2;
			 /*盒子阴影  x轴 y轴 羽化与半径 颜色*/
			 box-shadow:1px 1px 9px #222;
			 
		 }
		 #cate ul li img{
		     width:100%;
			 height:100%;
		 }
		 #cate ul li div{
		     position:absolute;
			 top:0;
			 left:0;
		     width:100%;
			 height:100%;
			 background:rgba(255,255,255,.5);
			 font-size:50px;
			 text-align:center;
			 opacity:0;
			 transform-origin:50% 50%;
			
		 }
         #cate ul li:hover div{
		     opacity:1;
			 animation:change .7s;
		 }
		 @keyframes change{
		     from{transform:scale(0)}
			 to{transform:scale(1)}
		 }
	  </style>
	  
